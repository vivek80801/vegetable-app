  "license": "MIT",
  "scripts": {
    "start": "node ./dist/bundle.min.js",
    "dev": "nodemon ./server.ts",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier .",
    "client:watch": "webpack --watch --config webpack.config.js",
    "server:watch": "webpack --watch --config webpack-client.config.js",
    "web:server": "webpack --config webpack.config.js",
    "web:client": "webpack --config webpack-client.config.js",
    "build": "concurrently \"yarn web:client\" \"yarn web:server\"",
    "watch": "concurrently \"yarn server:watch\" \"yarn client:watch\""
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/express-ejs-layouts": "^2.3.2",
    "@types/express-session": "^1.17.3",
    "@types/multer": "^1.4.5",
    "@types/passport": "^1.0.6",
    "@types/passport-local": "^1.0.33",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "concurrently": "^6.0.2",
    "css-loader": "^5.2.4",
    "ejs-loader": "^0.5.0",
    "eslint": "^7.25.0",
    "html-webpack-plugin": "^5.3.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "sass": "^1.32.12",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^9.1.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.6.0"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-ejs-layouts": "^2.5.0",
    "express-session": "^1.17.1",
    "mongodb-client-encryption": "^1.2.3",
    "mongoose": "^5.12.7",
    "multer": "^1.4.2",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0"
  }
}
